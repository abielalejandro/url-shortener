{
    "node_name": "consul-server",
    "server": true,
    "bootstrap" : true,
    "ui_config": {
        "enabled" : true
    }, 
    "datacenter": "dc1",
    "data_dir": "/opt/consul",
    "log_level":"INFO",
    "addresses": {
        "http" : "0.0.0.0"
    },
    "bind_addr": "0.0.0.0",
    "client_addr": "0.0.0.0",
    "connect": {
        "enabled": true
    },
    "ports": {
        "dns": 53
    },    
    "service": {
        "id": "dns",
        "name": "dns",
        "tags": ["primary"],
        "address": "localhost",
        "port": 53,
        "enable_tag_override": false,
        "check": {
          "id": "dns",
          "name": "Consul DNS TCP on port 53",
          "tcp": "localhost:53",
          "interval": "10s",
          "timeout": "1s"
        }
      }    
}